{
  "version": 1,
  "framework": {
    "name": "nextjs",
    "version": "15.5.0"
  },
  "imageOptimization": {
    "loader": "default"
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/",
      "status": 200
    }
  ],
  "buildCommand": "next build --experimental-build-mode=compile",
  "outputDirectory": ".next"
}