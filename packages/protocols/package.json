{
  "name": "@pixell/protocols",
  "version": "0.1.0",
  "description": "Agent-to-Agent (A2A) and Model Context Protocol (MCP) implementations for Pixell Agent Framework",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./a2a": {
      "types": "./dist/a2a/index.d.ts",
      "default": "./dist/a2a/index.js"
    },
    "./mcp": {
      "types": "./dist/mcp/index.d.ts",
      "default": "./dist/mcp/index.js"
    },
    "./shared": {
      "types": "./dist/shared/index.d.ts",
      "default": "./dist/shared/index.js"
    },
    "./plan-mode": {
      "types": "./dist/plan-mode/index.d.ts",
      "default": "./dist/plan-mode/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "jest"
  },
  "dependencies": {
    "zod": "^3.24.1",
    "uuid": "^10.0.0",
    "@types/uuid": "^10.0.0"
  },
  "devDependencies": {
    "typescript": "^5.7.3",
    "@types/node": "^22.10.5",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.13"
  },
  "keywords": [
    "ai",
    "agent",
    "protocol",
    "a2a",
    "mcp",
    "pixell"
  ],
  "author": "Pixell Contributors",
  "license": "Apache-2.0"
} 