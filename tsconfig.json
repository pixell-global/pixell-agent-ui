{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["esnext", "dom"],
    "module": "commonjs",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "include": ["**/*.ts", "**/*.tsx", "**/*.d.ts"]
}
