{
  "name": "pixell-agent-framework",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=web",
    "dev:orchestrator": "turbo dev --filter=orchestrator",
    "lint": "turbo lint",
    "test": "turbo test",
    "cli": "npm run build --workspace=@pixell/cli && node packages/cli/dist/index.js",
    "pxui": "npm run cli --",
    "env": "npm run cli -- env",
    "environments": "npm run cli -- env",
    "supabase:init": "npm run cli -- supabase init",
    "supabase:status": "npm run cli -- supabase status",
    "supabase:stop": "npm run cli -- supabase stop",
    "supabase:reset": "npm run cli -- supabase reset",
    "supabase:migrations": "npm run cli -- supabase migrations",
    "setup": "npm run setup:install && npm run setup:build:safe && npm run setup:link && npm run setup:env",
    "setup:complete": "npm run cli -- setup:complete",
    "check-deps": "npm run cli -- check-deps",
    "install-deps": "npm run cli -- install-deps",
    "setup:build:safe": "npm run setup:packages || npm run build",
    "setup:core": "npm install && npm run setup:packages && cd packages/cli && npm link",
    "setup:install": "npm install",
    "setup:build": "npm run build",
    "setup:link": "cd packages/cli && npm link",
    "setup:env": "npm run cli -- env && npm run cli -- supabase init && npm run cli -- config ai",
    "setup:quick": "npm install && npm run build && cd packages/cli && npm link",
    "setup:fresh": "npm ci && npm run build && cd packages/cli && npm link",
    "setup:packages": "turbo build --filter=!web && cd packages/cli && npm link",
    "start": "npm run dev",
    "pxui:start": "npm run cli -- start",
    "paf-core-agent:clone": "npm run cli -- paf-core-agent:clone",
    "paf-core-agent:update": "npm run cli -- paf-core-agent:update",
    "paf-core-agent:status": "npm run cli -- paf-core-agent:status",
    "paf-core-agent:remove": "npm run cli -- paf-core-agent:remove",
    "services:start": "npm run cli -- services:start",
    "services:stop": "npm run cli -- services:stop",
    "services:restart": "npm run cli -- services:restart",
    "services:status": "npm run cli -- services:status",
    "services:logs": "npm run cli -- services:logs",
    "docker:status": "npm run cli -- docker:status",
    "docker:start": "npm run cli -- docker:start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.8",
    "@types/mustache": "^4.2.6",
    "supabase": "^2.26.9",
    "turbo": "^2.5.4"
  },
  "packageManager": "npm@10.8.2",
  "dependencies": {
    "build": "^0.1.4",
    "mustache": "^4.2.0",
    "zod": "^3.25.75",
    "zustand": "^5.0.6"
  }
}
